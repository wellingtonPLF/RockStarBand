#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/wellingtonplf/Desktop/_Personal_Work/RockStarBand/BackEnd/node_modules/.pnpm/prisma-dbml-generator@0.12.0/node_modules/prisma-dbml-generator/dist/node_modules:/home/wellingtonplf/Desktop/_Personal_Work/RockStarBand/BackEnd/node_modules/.pnpm/prisma-dbml-generator@0.12.0/node_modules/prisma-dbml-generator/node_modules:/home/wellingtonplf/Desktop/_Personal_Work/RockStarBand/BackEnd/node_modules/.pnpm/prisma-dbml-generator@0.12.0/node_modules:/home/wellingtonplf/Desktop/_Personal_Work/RockStarBand/BackEnd/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/wellingtonplf/Desktop/_Personal_Work/RockStarBand/BackEnd/node_modules/.pnpm/prisma-dbml-generator@0.12.0/node_modules/prisma-dbml-generator/dist/node_modules:/home/wellingtonplf/Desktop/_Personal_Work/RockStarBand/BackEnd/node_modules/.pnpm/prisma-dbml-generator@0.12.0/node_modules/prisma-dbml-generator/node_modules:/home/wellingtonplf/Desktop/_Personal_Work/RockStarBand/BackEnd/node_modules/.pnpm/prisma-dbml-generator@0.12.0/node_modules:/home/wellingtonplf/Desktop/_Personal_Work/RockStarBand/BackEnd/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/generator.js" "$@"
else
  exec node  "$basedir/../../dist/generator.js" "$@"
fi
